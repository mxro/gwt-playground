<?xml version="1.0" encoding="UTF-8"?>
<!-- used for Local tests -->
<module rename-to='playground'>

	<inherits name="com.google.gwt.core.Core" />
	<inherits name='com.google.gwt.user.User' />
	<inherits name="org.timepedia.exporter.Exporter" />
	<set-property name="export" value="yes" />
	
	<inherits name ='delight.gwt.console.Module' />

	<!-- Specify the app entry point class. -->
	<entry-point
		class='de.mxro.gwtplayground.internal.PlaygroundEntryPoint' />

	<!-- To save time for compilation, create no permutations 
	<collapse-all-properties />-->
	
	<set-property name="user.agent" value="safari" />  <!-- only for Chrome --> 
	
	 <set-property name="compiler.stackMode" value="emulated" />
	<set-configuration-property name="compiler.emulatedStack.recordLineNumbers"
		value="true" />
	<set-configuration-property name="compiler.emulatedStack.recordFileNames"
		value="true" /> 

	<!-- Specify the paths for translatable code -->
	<source path='' />
	
	
	<add-linker name="xsiframe" /> 

	
</module>
